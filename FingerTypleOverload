/*
Test overload =
Overload = to display value
NOTE:
We can not change number of operand for operation
= is alway binaray 
++ and -- is alway unary operators
*/

#include <iostream>
using namespace std;


class Weird {

    private:
        int number;
    public:
        
        Weird(int n) {
            number = n;
        }

        void operator=(const Weird & obj){

            number = obj.number;
            cout << number;

        }

};

int main() {

    Weird test1(10), test2(5);

    test1 = test2;
    cout << endl;
    test2 = test1;

}


